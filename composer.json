  "description": "Harry Potter theme for flarum Forum.",
  "keywords": [
      "theme",
      "flarum",
      "harry potter"
  ],
  "type": "flarum-extension",
  "authors": [
      {
          "name": "Shahiem Seymor",
          "email": "s.seymor@live.nl",
          "homepage": "https://shahiemseymor.com/"
      }
  ],
  "require": {
      "flarum/core": "^0.1.0-beta.7"
  },
  "extra": {
      "flarum-extension": {
          "title": "Potter Theme",
          "icon": {
              "name": "bolt",
              "backgroundColor": "#f4f4f4",
